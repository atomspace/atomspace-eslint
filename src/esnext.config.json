{
	"envs": ["es6"],
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		}
	},
	"rules": {
		"no-var": "error",
		"prefer-template": "error",
		"object-shorthand": "error",
		"no-dupe-class-members": "error",
		"dot-notation": ["error", { "allowKeywords": true }],
		"no-param-reassign": ["error", { "props": false }]
	}
}