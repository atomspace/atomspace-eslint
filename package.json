  "repository": {
    "type": "git",
    "url": "git+https://github.com/atomspaceodua/atomspace-eslint.git"
  },
  "keywords": [
    "neutrino",
    "neutrino-preset",
    "atomspace",
    "eslint"
  ],
  "author": "Atom Space",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/atomspaceodua/atomspace-eslint/issues"
  },
  "homepage": "https://github.com/atomspaceodua/atomspace-eslint#readme",
  "dependencies": {
    "@neutrinojs/eslint": "~8.3.0",
    "eslint": "^4.19.1",
    "eslint-plugin-const-case": "~1.0.4",
    "eslint-plugin-es5": "~1.3.1",
    "eslint-plugin-eslint-comments": "~3.0.1",
    "eslint-plugin-filenames": "~1.3.2",
    "eslint-plugin-html": "~4.0.6",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "~21.27.1",
    "eslint-plugin-jsdoc": "~3.9.1",
    "eslint-plugin-markdown": "1.0.0",
    "eslint-plugin-optimize-regex": "~1.1.6",
    "eslint-plugin-promise": "~4.0.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-requirejs": "~3.2.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-restricted-globals": "0.2.0",
    "eslint-plugin-no-use-extend-native": "0.4.1",
    "eslint-plugin-react-hooks": "2.0.1"
  },
  "devDependencies": {
    "neutrino": "~8.3.0"
  },
  "peerDependencies": {
    "neutrino": "^8.0.0"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.0.0"
  },
  "publishConfig": {
    "access": "public"
