{
  "name": "@atomspace/eslint",
  "version": "3.1.1",
  "description": "Neutrino preset for adding ESLint config, following the Atom Space code style",
  "main": "src/index.js",
  "scripts": {
    "pretest": "neutrino lint",
    "lint": "neutrino lint --fix",
    "eslint": "eslint --ext .js,.jsx,.html,.md demo/"
