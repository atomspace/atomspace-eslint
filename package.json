{
	"name": "@atomspace/eslint",
	"version": "4.3.0",
	"description": "Neutrino preset for adding ESLint config, following the Atom Space code style",
	"main": "src/index.js",
	"scripts": {
		"eslint": "eslint ./src --ext .js,.jsx,.html,.md",
		"pretest": "npm run eslint -- --max-warnings 0 --format codeframe",
		"lint": "npm run eslint -- --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/atomspaceodua/atomspace-eslint.git"
	},
	"keywords": [
		"neutrino",
		"neutrino-preset",
		"atomspace",
		"eslint"
	],
	"author": "Atom Space",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/atomspaceodua/atomspace-eslint/issues"
	},
	"homepage": "https://github.com/atomspaceodua/atomspace-eslint#readme",
	"dependencies": {
		"@getify/eslint-plugin-proper-arrows": "8.0.1",
		"@neutrinojs/eslint": "9.1.0",
		"deepmerge": "4.2.2",
		"eslint-plugin-builtin-compat": "0.0.2",
		"eslint-plugin-const-case": "1.2.2",
		"eslint-plugin-es5": "1.5.0",
		"eslint-plugin-eslint-comments": "3.1.2",
		"eslint-plugin-eslint-plugin": "2.2.1",
		"eslint-plugin-filenames": "1.3.2",
		"eslint-plugin-html": "6.0.1",
		"eslint-plugin-import": "2.20.2",
		"eslint-plugin-jest": "23.8.2",
		"eslint-plugin-jsdoc": "23.0.0",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-lodash": "6.0.0",
		"eslint-plugin-lodash-fp": "2.2.0-a1",
		"eslint-plugin-markdown": "1.0.2",
		"eslint-plugin-new-with-error": "2.0.0",
		"eslint-plugin-no-secrets": "0.6.5",
		"eslint-plugin-no-use-extend-native": "0.5.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-optimize-regex": "1.1.7",
		"eslint-plugin-promise": "4.2.1",
		"eslint-plugin-ramda": "2.5.1",
		"eslint-plugin-react": "7.19.0",
		"eslint-plugin-react-hooks": "3.0.0",
		"eslint-plugin-requirejs": "4.0.0",
		"eslint-plugin-xss": "0.1.10",
		"eslint-restricted-globals": "0.2.0"
	},
	"devDependencies": {
		"eslint": "^6.8.0",
		"neutrino": "~9.1.0"
	},
	"peerDependencies": {
		"eslint": "^6.8.0",
		"neutrino": "^9.0.0"
	},
	"engines": {
		"node": ">=10",
		"npm": ">=5.4.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src",
		"LICENSE",
		"README.md",
		"package.json"
	]
}
